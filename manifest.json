{
  "manifest_version": 3,
  "name": "frist.io",
  "description": "React to any page on the internet.",
   "action": {
      "default_popup": "popup.html"
   },
  "permissions": ["activeTab", "storage"],
  "version": "0.1.0",
  "icons": {
    "16": "icons/veresk-16.png",
    "32": "icons/veresk-32.png",
    "48": "icons/veresk-48.png",
    "128": "icons/veresk-128.png"
  }
}
